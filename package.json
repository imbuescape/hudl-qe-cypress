{
  "name": "hudl-qe-project-cy",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run --env allure=true",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imbuescape/hudl-qe-cypress"
  },
  "keywords": [
    "cucumber",
    "cypress",
    "BDD"
  ],
  "author": "John Oni",
  "cypress-cucumber-preprocessor": {
  "stepDefinitions": "cypress/e2e/step_definitions/*.ts"
  },
  "experimentalOriginDependencies": true,
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^24.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.8",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@faker-js/faker": "^10.2.0",
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "allure-cypress": "^3.4.5",
    "cypress": "^15.10.0",
    "eslint": "^10.0.0",
    "eslint-formatter-codeframe": "^7.32.2",
    "eslint-plugin-cypress": "^5.2.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "typescript": "^5.9.3"
  },
   "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "dotenv": "^17.2.4"
  }
}
